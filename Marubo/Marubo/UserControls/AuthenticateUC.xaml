<UserControl x:Class="Marubo.UserControls.AuthenticateUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Marubo.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <Button x:Name="BtnCreateUser" Content="Opret bruger" Click="BtnCreateUser_Click" Style="{StaticResource AuthWindowButton}"/>
            <Button x:Name="BtnLogin" Content="Log ind" Click="BtnLogin_Click" Margin="0,10,0,0" Style="{StaticResource AuthWindowButton}"/>
        </StackPanel>
    </Grid>
</UserControl>
